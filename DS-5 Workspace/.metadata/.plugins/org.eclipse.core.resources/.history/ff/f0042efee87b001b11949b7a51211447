#include "serial.h"
int main(void){
    Init_BT();
    BT_Flush();

    printf("Bluetooth initialized\n");

    BTOutMessage("Hello from arm processor");

	while(1){
		char data_r = getcharBT();
		printf("receive in ascii:(hex) %X,(char) %c\n",data_r,data_r);
	}


}
